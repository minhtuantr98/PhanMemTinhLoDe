        function getListVatLieu() {
            $.ajax({
                type: "POST",
                async: false,
                url: '/TinhToan/GetListBeTong',
                success: function (response) {
                    var rs = JSON.parse(response);
                    console.log("data", rs);
                    if (rs != []) {
                        var html_str = "";
                        for (var i = 0; i < rs.length; i++) {
                            var obj = rs[i];

                            html_str += "<option ";
                            //if (id != "") {
                            //	if (id == obj.ValueMember) {
                            //		html_str += "selected ";
                            //	}
                            //}
                            html_str += "value = '" + obj.ID + "' > " + obj.Name + "</option > ";
                        }
                        $("#macBeTong").html(html_str);
                        $("#Rb1").html(rs[0].CuongDoNen);
                        $("#Rbt").html(rs[0].CuongDoKeo);
                        $("#Ebt").html(rs[0].ModuleDanHoi);
                    }
                },
                error: function (rs) {
                    console.log(rs);
                    //toastr["error"]("Có lỗi xảy ra trong quá trình lấy danh sách vật liệu. Vui lòng thử lại sau");
                }
            });

