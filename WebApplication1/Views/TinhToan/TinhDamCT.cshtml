        function getDetail(id, type) {
            $.ajax({
                type: "POST",
                async: false,
                url: '/TinhToan/GetDetailVatLieu',
                data: { id: id },
                success: function (response) {
                    var rs = JSON.parse(response);
                    console.log("data", rs);
                    if (rs != []) {
                        var obj = rs[0];
                        var html_str = "";
                        switch (type) {
                            case "BT":
                                $("#Rb1").html(obj.CuongDoNen);
                                $("#Rbt").html(obj.CuongDoKeo);
                                $("#Ebt").html(obj.ModuleDanHoi);
                                break;
                            case "TD":
                                $("#Rs").html(obj.CuongDoNen);
                                $("#Rsc").html(obj.CuongDoKeo);
                                break;
                            case "TDD":
                                $("#Rsw").html(obj.CuongDoKeo);
                                $("#Es").html(obj.ModuleDanHoi);
                                break;
                            default:
                        }
                    }
                },
                error: function (rs) {
                    console.log(rs);
                    //toastr["error"]("Có lỗi xảy ra trong quá trình lấy danh sách vật liệu. Vui lòng thử lại sau");
                }
            });
        }

