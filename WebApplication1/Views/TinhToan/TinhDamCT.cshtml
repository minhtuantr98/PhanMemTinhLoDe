                success: function (response) {
                    var rs = JSON.parse(response);
                    console.log("data", rs);
                    if (rs != []) {
                        var html_str = "";
                        for (var i = 0; i < rs.length; i++) {
                            var obj = rs[i];

                            html_str += "<option ";
                            html_str += "value = '" + obj.ID + "' > " + obj.Name + "</option > ";
                        }
                        $("#macThepDoc").html(html_str);
                        $("#Rs").html(rs[0].CuongDoNen);
                        $("#Rsc").html(rs[0].CuongDoKeo);
                    }
                },
